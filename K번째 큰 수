w, h = map(int,input().split())
arr = [[0 for j in range(h)] for i in range(w)]
n = int(input())
for z in range(0, n):
    l, d, x, y = map(int,input().split())
    x-=1
    y-=1
    for i in range(l):
        if(d == 0) :
            arr[x][y] = 1
            arr[x] = arr[x] + 1
            
        if(d == 1) :
            arr[x][y] = 1
            arr[y] = arr[y] + 1
for i in range(w):
    for j in range(h):
        print(arr[i][j], end=' ')
    print()
